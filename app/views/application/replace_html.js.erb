<% if !(defined? hideElem).nil? %>
$('#<%= hideElem %>').hide();
<% end %>
$('#<%= replaceElem %>').html('<%= escape_javascript render(:partial => partial, :object => object, :locals => locals)%>');
$('.dropdownmenu').dropdownmenu();
$( "#tabs" ).tabs();
$( "#dialog" ).dialog({modal: true, appendTo: "#modal_dialog"});
